conda create -n lsam python=3.11
conda activate lsam
conda install pytorch==2.4.1 torchvision==0.19.1 cuda-nvcc -c nvidia -c pytorch

pip install torch==2.4.1 torchvision==0.19.1 --extra-index-url https://download.pytorch.org/whl/cu124

conda install nvidia::cuda-nvcc -y
sudo find / -name nvcc
#ser CUDA_HOME where nvcc is installed on conda env
pip install -U git+https://github.com/luca-medeiros/lang-segment-anything.git
#iterative install dev version of missing packages
